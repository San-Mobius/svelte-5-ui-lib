@tailwind base;

@layer base {
	#mainContent>p{
		@apply leading-relaxed mb-4;
	}
	code{
		@apply text-primary-800 dark:text-primary-500;
	}
}

@tailwind components;

@layer components {
	/* other classes */

	.container {
		@apply m-4;
	}
}

@tailwind utilities;

